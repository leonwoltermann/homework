<milestone unit="sentence" n="893" /> --Flour active, with large sales — <persName n="Howard,,,,," id="n-0001.0000.00000.00580" reg="mostcommon:Howard,nomatch:0" authname="howard"><surname full="yes">Howard</surname></persName> <measure n="5.25dollars" type="currency">$5.25</measure>. <persName n="Wheat,,,,," id="n-0001.0000.00000.00581" reg="mostcommon:Wheat,nomatch:0" authname="wheat"><surname full="yes">Wheat</surname></persName> dull — red <measure n="1.25dollars" type="currency">$1.25</measure>@<num value="1.33">1.33</num>; white <measure n="1.40dollars" type="currency">$1.40</measure>@<num value="1.65">1.65</num>.

