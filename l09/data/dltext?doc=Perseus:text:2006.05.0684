<item><persName n="Barnett,,,,," id="n-0001.0000.00000.00027" reg="mostcommon:Barnett,nomatch:0" authname="barnett"><surname full="yes">Barnett</surname></persName> </item> 
<item><unclear /> </item> 
<item><unclear /> </item> 
<item><unclear /> </item> 
<item><persName n="Elliott,,,,," id="n-0001.0000.00000.00028" reg="mostcommon:Elliott,nomatch:0" authname="elliott"><surname full="yes">Elliott</surname></persName>, </item> 
<item>Miles </item> 
<item><placeName key="tgn,6002055" n="1.000 83" reg="fort henry, stewart, tennessee" authname="tgn,6002055">Henry</placeName> </item> 
<item><unclear /> </item> 
<item><placeName reg="Wright, Lake, Tennessee" key="tgn,2783061" authname="tgn,2783061">Wright, Tenn.</placeName>, </item> 
<item><unclear /> <rs type="role2">Miss</rs>, </item> 
<item>Matches, </item> 
<item><placeName key="tgn,7002435" n="1.000 184" reg="rossiya" authname="tgn,7002435">Russia</placeName>, </item> 
<item><unclear /> </item> 
<item><persName n="Bruce,,,,," id="n-0001.0000.00000.00029" reg="mostcommon:Bruce,nomatch:0" authname="bruce"><surname full="yes">Bruce</surname></persName>, </item> 
<item><persName n="Lyons,,,,," id="n-0001.0000.00000.00030" reg="mostcommon:Lyons,nomatch:0" authname="lyons"><surname full="yes">Lyons</surname></persName>, </item> 
<item><persName n="Johnson,,,,," id="n-0001.0000.00000.00031" reg="mostcommon:Johnson,nomatch:0" authname="johnson"><surname full="yes">Johnson</surname></persName>, </item> 
<item><unclear>Cacke,</unclear> </item> 
