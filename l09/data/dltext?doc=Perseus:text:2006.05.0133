<milestone unit="sentence" n="546" />For amendment, <num value="236">236</num>; against it, <num value="103">103</num>.

<milestone unit="sentence" n="547" />For Senate <persName n="Harvey,,Joseph,F.,," id="n-0001.0000.00000.00283" reg="default:Harvey,Joseph,F.,," authname="harvey,joseph,f."><foreName n="Joseph" full="yes">Jos.</foreName> <foreName full="yes">F.</foreName> <surname full="yes">Harvey</surname></persName>, (no opposition,) <num value="382">382</num>. <orgName n="House of Delegates" type="government">House of Delegates</orgName>--<persName n="Carter,,A.,L.,," id="n-0001.0000.00000.00284" reg="default:Carter,A.,L.,," authname="carter,a.,l."><foreName full="yes">A.</foreName> <foreName full="yes">L.</foreName> <surname full="yes">Carter</surname></persName>, (no opposition,) <num value="388">388</num>. <orgName n="Public Works Board" type="board">Board of Public Works</orgName>--<persName n="Holladay,,Alexander,,," id="n-0001.0000.00000.00285" reg="default:Holladay,Alexander,,," authname="holladay,alexander"><foreName n="Alexander" full="yes">Alex.</foreName> <surname full="yes">Holladay</surname></persName>, <num value="84">84</num>; <persName n="Broun,,T.,L.,," id="n-0001.0000.00000.00286" reg="default:Broun,T.,L.,," authname="broun,t.,l."><foreName full="yes">T.</foreName> <foreName full="yes">L.</foreName> <surname full="yes">Broun</surname></persName>, <num value="75">75</num>. </p> </div4> 
