<item>J T <persName n="Vaughan,,,,," id="n-0001.0000.00000.00179" reg="mostcommon:Vaughan,nomatch:0" authname="vaughan"><surname full="yes">Vaughan</surname></persName>, </item> 
<item><persName n="Tucker,,Thos,,," id="n-0001.0000.00000.00180" reg="default:Tucker,Thos,,," authname="tucker,thos"><foreName full="yes">Thos</foreName> <surname full="yes">Tucker</surname></persName>, </item> 
<item><persName n="Wright,,Carter,,," id="n-0001.0000.00000.00181" reg="default:Wright,Carter,,," authname="wright,carter"><foreName full="yes">Carter</foreName> <surname full="yes">Wright</surname></persName>, </item> 
<item>Wm F Breadlow, </item> 
<item><persName n="Maloy,,James,,," id="n-0001.0000.00000.00182" reg="default:Maloy,James,,," authname="maloy,james"><foreName full="yes">James</foreName> <surname full="yes">Maloy</surname></persName>, </item> 
