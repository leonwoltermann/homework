<item>Jumius B <persName n="Thomas,,,,," id="n-0001.0000.00000.00169" reg="mostcommon:Thomas,nomatch:0" authname="thomas"><surname full="yes">Thomas</surname></persName>, </item> 
<item><persName n="Pittman,,Elijah,,," id="n-0001.0000.00000.00170" reg="default:Pittman,Elijah,,," authname="pittman,elijah"><foreName full="yes">Elijah</foreName> <surname full="yes">Pittman</surname></persName>, </item> 
