<milestone unit="sentence" n="682" />Wounded — <persName n="Bird,,John,W.,," id="n-0001.0000.00000.00328" reg="default:Bird,John,W.,," authname="bird,john,w."><foreName full="yes">John</foreName> <foreName full="yes">W.</foreName> <surname full="yes">Bird</surname></persName>, <persName n="McLaughlin,,Robert,,," id="n-0001.0000.00000.00329" reg="default:McLaughlin,Robert,,," authname="mclaughlin,robert"><foreName full="yes">Robert</foreName> <surname full="yes">McLaughlin</surname></persName>, and <persName n="Pullins,,James,,," id="n-0001.0000.00000.00330" reg="default:Pullins,James,,," authname="pullins,james"><foreName full="yes">James</foreName> <surname full="yes">Pullins</surname></persName>.

