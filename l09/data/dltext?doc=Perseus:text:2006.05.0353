<row role="data"><cell role="data" rows="1" cols="1">N. American and W. I.</cell><cell role="data" rows="1" cols="1"><num value="30">30</num></cell><cell role="data" rows="1" cols="1"><num value="714">714</num></cell><cell role="data" rows="1" cols="1"><num value="8075">8,075</num></cell></row> 
