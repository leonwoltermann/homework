<milestone unit="sentence" n="680" />Wounded — <persName n="Pridmore,,John,,," id="n-0001.0000.00000.00321" reg="default:Pridmore,John,,," authname="pridmore,john"><foreName full="yes">John</foreName> <surname full="yes">Pridmore</surname></persName>, <persName n="Taylor,,William,S.,," id="n-0001.0000.00000.00322" reg="default:Taylor,William,S.,," authname="taylor,william,s."><foreName full="yes">William</foreName> <foreName full="yes">S.</foreName> <surname full="yes">Taylor</surname></persName>, <persName n="Granville,,,,," id="n-0001.0000.00000.00323" reg="mostcommon:Granville,nomatch:0" authname="granville"><surname full="yes">Granville</surname></persName> C <placeName reg="Lake Octerman Bond">Lake Octerman Bond</placeName>, <persName n="Sims,,Alfred,,," id="n-0001.0000.00000.00324" reg="default:Sims,Alfred,,," authname="sims,alfred"><foreName full="yes">Alfred</foreName> <surname full="yes">Sims</surname></persName>, <persName n="Snider,,Joseph,C.,," id="n-0001.0000.00000.00325" reg="default:Snider,Joseph,C.,," authname="snider,joseph,c."><foreName full="yes">Joseph</foreName> <foreName full="yes">C.</foreName> <surname full="yes">Snider</surname></persName>, and <persName n="Dawson,,Martin,L.,," id="n-0001.0000.00000.00326" reg="default:Dawson,Martin,L.,," authname="dawson,martin,l."><foreName full="yes">Martin</foreName> <foreName full="yes">L.</foreName> <surname full="yes">Dawson</surname></persName>. </item> 
