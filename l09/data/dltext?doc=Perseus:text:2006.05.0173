<milestone unit="sentence" n="450" /></item> </list> <closer> By order <signed><persName n="Moore,,P.,T.,," id="n-0001.0000.00000.00171" reg="default:Moore,P.,T.,," authname="moore,p.,t."><foreName full="yes">P.</foreName> <foreName full="yes">T.</foreName>  <surname full="yes">Moore</surname></persName>, <persName n="Com,Colonel,,,," id="n-0001.0000.00000.00172" reg="mostcommon:Com,nomatch:0" authname="com"><roleName n="Colonel" full="yes">Col.</roleName> <surname full="yes">Com</surname></persName>'g.</signed> <lb /> <signed><persName n="Otey,,G.,G.,," id="n-0001.0000.00000.00173" reg="default:Otey,G.,G.,," authname="otey,g.,g."><foreName full="yes">G.</foreName> <foreName full="yes">G.</foreName>  <surname full="yes">Otey</surname></persName>, Adjutant.</signed> <term type="printrun">de 21--2t</term> </closer> </div3> </div2> 
