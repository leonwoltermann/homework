<item><persName n="Hunt,,Washington,,," id="n-0001.0000.00000.00075" reg="default:Hunt,Washington,,," authname="hunt,washington"><foreName full="yes">Washington</foreName> <surname full="yes">Hunt</surname></persName>, </item> 
