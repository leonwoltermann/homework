<p>

<milestone unit="sentence" n="309" /> <placeName reg="Brooklyn, New York, Kings" key="tgn,7015822" authname="tgn,7015822">Brooklyn</placeName> — <persName n="Breckinridge,,,,," id="n-0001.0000.00000.00144" reg="mostcommon:Breckinridge,nomatch:0" authname="breckinridge"><surname full="yes">Breckinridge</surname></persName> <num value="53">53</num>; <persName n="Bell,,,,," id="n-0001.0000.00000.00145" reg="mostcommon:Bell,nomatch:0" authname="bell"><surname full="yes">Bell</surname></persName> <num value="45">45</num>; <persName n="Douglas,,,,," id="n-0001.0000.00000.00146" reg="mostcommon:Douglas,nomatch:0" authname="douglas"><surname full="yes">Douglas</surname></persName> <num value="7">7</num>. <placeName reg="Mt. Carmel-- Breckinridge">Mt. Carmel-- Breckinridge</placeName> <num value="91">91</num>; <persName n="Bell,,,,," id="n-0001.0000.00000.00147" reg="mostcommon:Bell,nomatch:0" authname="bell"><surname full="yes">Bell</surname></persName> <num value="46">46</num>; <persName n="Douglas,,,,," id="n-0001.0000.00000.00148" reg="mostcommon:Douglas,nomatch:0" authname="douglas"><surname full="yes">Douglas</surname></persName> <num value="4">4</num>.

